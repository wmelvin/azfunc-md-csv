{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "inblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "fileup/{name}.csv",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outblob",
      "type": "blob",
      "direction": "out",
      "path": "output-md/{name}.md",
      "connection": "AzureWebJobsStorage"
    }
  ]
}